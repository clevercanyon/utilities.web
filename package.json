{
	"private": false,
	"publishConfig": {
		"access": "restricted"
	},
	"version": "1.0.25",
	"license": "GPL-3.0-or-later",
	"name": "@clevercanyon/utilities.web",
	"description": "Another great project by Clever Canyon.",
	"repository": "https://github.com/clevercanyon/utilities.web",
	"homepage": "https://github.com/clevercanyon/utilities.web#readme",
	"bugs": "https://github.com/clevercanyon/utilities.web/issues",
	"funding": "https://github.com/sponsors/clevercanyon",
	"keywords": ["clevercanyon"],
	"author": {
		"name": "Clever Canyon",
		"url": "https://clevercanyon.com"
	},
	"contributors": [],
	"type": "module",
	"files": ["/dist"],
	"bin": {},
	"imports": {},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/types/index.d.ts"
		},
		"./analytics": {
			"import": "./dist/analytics.js",
			"require": "./dist/analytics.cjs",
			"types": "./dist/types/analytics.d.ts"
		},
		"./dom": {
			"import": "./dist/dom.js",
			"require": "./dist/dom.cjs",
			"types": "./dist/types/dom.d.ts"
		}
	},
	"sideEffects": ["./src/*.{html,scss,css,tsx,ts,jsx,mjs,js}"],
	"module": "./dist/index.js",
	"main": "./dist/index.cjs",
	"browser": "./dist/index.js",
	"unpkg": "./dist/index.js",
	"types": "./dist/types/index.d.ts",
	"typesVersions": {
		">=3.1": {
			"./*": ["./dist/types/*"]
		}
	},
	"peerDependencies": {
		"@clevercanyon/utilities": "^1.0.75"
	},
	"peerDependenciesMeta": {},
	"bundleDependencies": [],
	"devDependencies": {
		"@clevercanyon/skeleton-dev-deps": "^1.0.83"
	},
	"overrides": {},
	"cpu": ["x64", "arm64"],
	"os": ["darwin", "linux"],
	"engines": {
		"node": "^19.2.0 || ^19.4.0",
		"npm": "^8.19.3 || ^9.2.0"
	},
	"workspaces": [],
	"config": {
		"c10n": {
			"&": {
				"dotfiles": {
					"lock": []
				},
				"build": {
					"appType": "cma",
					"targetEnv": "web"
				},
				"github": {
					"teams": {
						"engineers": "pull",
						"architects": "push"
					},
					"configVersion": "1.0.0"
				},
				"npmjs": {
					"configVersions": "1.0.0,1.0.0"
				}
			}
		}
	}
}
