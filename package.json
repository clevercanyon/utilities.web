{
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "version": "1.0.84",
    "license": "GPL-3.0-or-later",
    "name": "@clevercanyon/utilities.web",
    "description": "Utilities for JavaScript apps running in a web environment.",
    "repository": "https://github.com/clevercanyon/utilities.web",
    "homepage": "https://github.com/clevercanyon/utilities.web#readme",
    "bugs": "https://github.com/clevercanyon/utilities.web/issues",
    "funding": "https://github.com/sponsors/clevercanyon",
    "keywords": ["clevercanyon"],
    "author": {
        "name": "Clever Canyon",
        "url": "https://clevercanyon.com"
    },
    "contributors": [],
    "type": "module",
    "bin": {},
    "imports": {},
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./analytics": {
            "types": "./dist/types/analytics.d.ts",
            "import": "./dist/analytics.js",
            "default": "./dist/analytics.js"
        },
        "./dom": {
            "types": "./dist/types/dom.d.ts",
            "import": "./dist/dom.js",
            "default": "./dist/dom.js"
        }
    },
    "sideEffects": ["**/*.{css,scss,less}", "./src/resources/init.ts"],
    "module": "./dist/index.js",
    "main": "./dist/index.js",
    "browser": "./dist/index.js",
    "unpkg": "./dist/index.js",
    "types": "./dist/types/index.d.ts",
    "typesVersions": {
        ">=3.1": {
            "./*": ["./dist/types/*"]
        }
    },
    "dependencies": {},
    "peerDependencies": {
        "@clevercanyon/utilities": "^1.0.332"
    },
    "peerDependenciesMeta": {},
    "optionalDependencies": {},
    "bundleDependencies": [],
    "devDependencies": {
        "@clevercanyon/dev-deps": "^1.0.249"
    },
    "overrides": {},
    "cpu": ["x64", "arm64"],
    "os": ["darwin", "linux"],
    "engines": {
        "node": "^19.1.0 || ^20.5.0",
        "npm": "^8.19.3 || ^9.8.0 || ^10.0.0"
    },
    "config": {
        "c10n": {
            "&": {
                "dotfiles": {
                    "lock": []
                },
                "build": {
                    "appType": "lib",
                    "targetEnv": "web",
                    "entryFiles": [],
                    "sideEffects": []
                },
                "ssrBuild": {
                    "appType": "",
                    "targetEnv": "",
                    "entryFiles": [],
                    "sideEffects": []
                },
                "github": {
                    "teams": {
                        "engineers": "pull",
                        "architects": "push"
                    },
                    "labels": {},
                    "configVersion": "1.0.9",
                    "envsVersion": ""
                },
                "npmjs": {
                    "configVersions": "1.0.9,1.0.9"
                }
            }
        }
    }
}
